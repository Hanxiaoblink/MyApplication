  <declare-styleable name="PullToZoomView">
    <attr name="headerView" format="reference"/>
    <attr name="contentView" format="reference"/>
    <attr name="zoomView" format="reference"/>
    <attr name="isHeaderParallax" format="boolean"/>
  </declare-styleable>
  <!-- StrokedTextView specific attributes. -->
  <declare-styleable name="StrokedTextView">
    <!-- The color of the stroke outline -->
    <attr name="strokeViewColor" format="reference|color"/>
    <!-- The color of the text -->
    <attr name="strokeTextColor" format="reference|color"/>
    <!-- The width of the stroke -->
    <attr name="strokeOutlineWidth" format="reference|float"/>

    <attr name="gradient" format="reference|boolean"/>
    <attr name="startColor" format="reference|color"/>
    <attr name="endColor" format="reference|color"/>
    <attr name="text_bold" format="reference|boolean"/>
    <attr name="shadow_color" format="reference|color"/>
    <attr name="text_font">
      <enum name="aff_bold" value="1"/>
      <enum name="aff_black" value="2"/>
      <enum name="baskerville" value="3"/>
      <enum name="baskerville_bold_mini" value="4"/>
      <enum name="fire_car" value="5"/>
      <enum name="game_fb_result_score_num" value="6"/>
      <enum name="game_fb_result_text" value="7"/>
      <enum name="georgia_mini" value="8"/>
      <enum name="super_car_title" value="9"/>
      <enum name="yuanti_001_status" value="10"/>
    </attr>
  </declare-styleable>

  <declare-styleable name="LevelProgress">
